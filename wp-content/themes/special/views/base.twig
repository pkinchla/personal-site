  {% block html_head_container %}

  {% include 'html-header.twig' %}
    {% block head %}
    {% endblock %}
    </head>
  {% endblock %}

  <body class="{{body_class}}" data-template="base.twig">
    <div id="page" class="hfeed site site-grid">
      <a class="skip-link assistive-text" href="#content">Skip To Content</a>
      {% block header %}
      <header id="masthead" class="site-header">
        {% include 'site-logo.twig' %}
        <nav id="site-navigation" class="site-navigation js-main-navigation">
          {% include "menu.twig" with {'menu': menu.get_items} %}
        </nav><!-- #site-navigation -->
      </header><!-- #masthead -->
      {% endblock %}

      <main id="content" class="site-content">
        {% block content %}
        {% endblock %}
      </main><!-- #content -->

      {% block footer %}
        <footer id="colophon" class="site-footer">
          {% include 'footer.twig' %}
        </footer>
        {{ function('wp_footer') }}
      {% endblock %}
    </div><!-- #page -->
  </body>
</html>
