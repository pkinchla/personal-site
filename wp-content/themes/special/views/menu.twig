{% if menu %}
  <ul id="primary-menu" role="list">
  {% for item in menu %}
    <li>
      <a class="no-offset" href="{{item.get_link}}" aria-current="{{ (item.title|lower == menu_type) ? 'true' : 'false' }}"><span>{{item.title}}</span></a>
      {% include "menu.twig" with {'menu': item.get_children} %}
    </li>
  {% endfor %}
  </ul>
{% endif %}